{
    "sap.app": {
        "id": "fop.template.ui5",
        "type": "application"
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "rootView": {
            "type": "XML",
            "async": true,
            "viewName": "fop.template.ui5.view.App"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": { "bundleName": "fop.template.ui5.i18n.i18n" }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "fop.template.ui5.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "show"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "first",
                    "target": "firstPage"
                },
                {
                    "pattern": "second",
                    "name": "second",
                    "target": "secondPage"
                }
            ],
            "targets": {
                "firstPage": {
                    "viewId": "FirstPage",
                    "viewName": "FirstPage"
                },
                "secondPage": {
                    "viewId": "SecondPage",
                    "viewName": "SecondPage"
                }
            }
        }
    }
}